{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AdaptationAuditLogLine",
  "type": "object",
  "required": [
    "session_id",
    "inputs",
    "output",
    "config_version_hash",
    "seed",
    "intermediate",
    "timestamp_utc"
  ],
  "properties": {
    "session_id": { "type": "string" },
    "timestamp_utc": { "type": "string" },
    "inputs": { "$ref": "event.schema.json" },
    "output": { "$ref": "decision.schema.json" },
    "config_version_hash": { "type": "string" },
    "seed": { "type": "integer" },
    "intermediate": {
      "type": "object",
      "additionalProperties": { "type": "number" }
    }
  },
  "additionalProperties": false
}
